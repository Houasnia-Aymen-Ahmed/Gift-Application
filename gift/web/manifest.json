{
	"manifest_version": 3,
	"name": "Gift App Extension",
	"version": "1.0",
	"description": "Access Gift App features from your browser.",
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "assets/assets/icon/Logo.png",
			"48": "assets/assets/icon/Logo.png",
			"128": "assets/assets/icon/logoForScreen.png"
		}
	},
	"icons": {
		"16": "assets/assets/icon/Logo.png",
		"48": "assets/assets/icon/Logo.png",
		"128": "assets/assets/icon/logoForScreen.png"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self' ; object-src 'self'"
	}
}